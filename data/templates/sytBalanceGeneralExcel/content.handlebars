<table>
    <tr>
        <th colspan="3">
            {{empresa}}
        </th>
    </tr>
    <tr>
        <th colspan="3">
            Balance General
        </th>
    </tr>    
    <tr>
        <th colspan="3">
            Al {{formatDate al}}
        </th>
    </tr>
    {{#each cuentas}}
    <tr>
        <td>{{nombrecta}}</td>
        <td>
            {{#shouldPrint codigo 7}}
                {{toFloat saldo}}
            {{else}}
            {{/shouldPrint}}
        </td>
        <td>
            {{#shouldPrint codigo 5}}
                {{toFloat saldo}}
            {{else}}
            {{/shouldPrint}}
        </td>
    </tr>
    {{/each}}
</table>