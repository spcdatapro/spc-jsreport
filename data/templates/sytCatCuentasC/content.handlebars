<html>
    <head>
        <meta content="text/html; charset=utf-8" http-equiv="Content-Type">
        <style>
            
            .tbl{ width: 100%; border-collapse: collapse; }
            .tipo{ font-family: Arial; font-size: 11pt;}
            .bld{ font-weight: bold; }
            .pdbott{ padding-bottom: 0.5em; }
            .ctxt{ text-align: center; }
        </style>
    </head>
    <body>
        <table class="tbl tipo">
            <tbody>
                {{#each cuentas}}
                <tr>
                    <td style="width: 15%" class="pdbott {{#estotales tipocuenta}}{{/estotales}}">{{codigo}}</td>
                    <td style="width: 85%" class="pdbott {{#estotales tipocuenta}}{{/estotales}}">{{nombrecta}}</td>
                </tr>
                {{/each}}
            </tbody>
        </table>
    </body>
</html>